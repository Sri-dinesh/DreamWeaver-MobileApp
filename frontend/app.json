{
  "expo": {
    "name": "DreamWeaver",
    "slug": "DreamWeaver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MainLogo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/MainLogo.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b8c270a5-4faf-4a10-b0c1-4e983dd51782"
      }
    },
    "android": {
      "package": "com.sridinesh.DreamWeaver"
    }
  }
}
